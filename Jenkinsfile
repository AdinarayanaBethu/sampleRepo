pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Checkout from Git'
            }
        }
       stage('Build') {
            steps {
                echo 'Build Complete'
            }
        }
       stage('Test') {
            steps {
                echo 'Testing Complete'
            }
        }
       stage('Deploy') {
            steps {
                echo 'Deployment Done'
            }
        }
    }
}
